<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzm.medicineapp.mapper.HealthMapper">
        <!--    public Health getHealthById(Integer id);-->
        <select id="getHealthById" resultType="health">
                select * from health_tb where id=#{id}
        </select>
        <!--public List<Health> getHealthsByType(@Param("main_type") String main_type,@Param("sub_type") String sub_type);-->
        <select id="getHealthsByType" resultType="health">
                select * from health_tb where main_type=#{main_type} and sub_type=#{sub_type}
        </select>
        <!--public List<Health> getHealths();-->
        <select id="getHealths" resultType="health">
                select * from health_tb
        </select>
</mapper>